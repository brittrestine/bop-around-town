<div class="container devise-container">
<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="field">
    <!-- < %= f.label :email %> -->
    <%= f.email_field :email, autofocus: true, placeholder: " enter email" %>
  </div>

  <div class="field field-password">
    <!-- < %= f.label :password %> -->
   <!--  < % if @minimum_password_length %>
    <em>(< % = @minimum_password_length %> characters minimum)</em>
    < % end %><br /> -->
    <%= f.password_field :password, autocomplete: "off" , placeholder: " password" %>
  </div>

  <div class="field field-password">
    <!-- < %= f.label :password_confirmation %><br /> -->
    <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: " password confirm" %>
  </div>

  <div class="actions">
    <%= f.submit "Sign up", id: "sign-up-but" %>
  </div>
<% end %>

<!-- < % = render "devise/shared/links" % >
 -->
</div>

