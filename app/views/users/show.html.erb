<div class="profile-page">
    <h1>welcome to your home page</h1>

    <h2>upcoming shows from your previous searches</h2>

    <% @upcoming_shows.each do |concert| %>
      <h2><%= concert[:genre] %></h2>
      <% concert[:shows].each do |show| %>
        <div id="user-show" class="five.columns">
          <%= render partial: 'homes/display_show', locals: { show: show } %>
        </div>
      <% end %>
    <% end %>

    <h6><%= link_to('edit account', edit_user_registration_path) %></h6>
</div>



